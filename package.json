{
  "name": "tsnip-schematic",
  "version": "0.0.0",
  "description": "Tsnip is an Angular Schematic to generate meaningful Test Snippets for existing CLI applications",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*_spec.js"
  },
  "keywords": [
    "schematics",
    "tsnip",
    "angular",
    "specs"
  ],
  "author": "Craig Ryan, Ryanfuse.",
  "contributors": [
    "Craig Ryan <cryan.dublin@gmail.com>"
  ],
  "license": "MIT",
  "schematics": "./src/collection.json",
  "devDependencies": {
    "@angular-devkit/core": "^7.1.3",
    "@angular-devkit/schematics": "^7.1.3",
    "@types/jasmine": "^2.6.0",
    "@types/node": "^8.0.31",
    "jasmine": "^2.8.0",
    "typescript": "~3.1.6"
  }
}
